else
  export NODE_PATH="$NODE_PATH:/Users/cfa/Developer/Code-From-Anywhere/HimalayaJeep/node_modules/.pnpm/turbo@1.1.5/node_modules/turbo/bin/node_modules:/Users/cfa/Developer/Code-From-Anywhere/HimalayaJeep/node_modules/.pnpm/turbo@1.1.5/node_modules/turbo/node_modules:/Users/cfa/Developer/Code-From-Anywhere/HimalayaJeep/node_modules/.pnpm/turbo@1.1.5/node_modules:/Users/cfa/Developer/Code-From-Anywhere/HimalayaJeep/node_modules/.pnpm/node_modules:/Users/cfa/Developer/Code-From-Anywhere/HimalayaJeep/node_modules:/Users/cfa/Developer/Code-From-Anywhere/node_modules:/Users/cfa/Developer/node_modules:/Users/cfa/node_modules:/Users/node_modules:/node_modules"
fi
